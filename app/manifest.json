{
    "name": "FunReader",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Simply enjoy your daily reads.",
    "browser_action": {
        "default_icon": "img/icon64.png",
        "default_popup": "index.html"
    },
    /*"app": {
        "launch": {
            "local_path": "index.html",
            "width": 800,
            "height": 6hhhh00,
            "container": "panel"
        }
    },*/
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "64": "img/icon64.png",
        "128": "img/icon128.png"
    },
    "permissions": [
        "http://reader.loc/*"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
    "web_accessible_resources": [
        "img/preloader.gif",
        "img/preloader2.gif"
    ]
}